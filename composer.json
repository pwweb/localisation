{
    "name": "pwweb/localisation",
    "description": "Localisation package for Laravel 6",
    "license": "MIT",
    "authors": [{
        "name": "Frank Pillukeit",
        "email": "clients@pw-websolutions.com",
        "homepage": "https://www.pw-websolutions.com"
    }],
    "homepage": "https://github.com/pwweb/localisation",
    "keywords": ["library", "laravel", "localisation", "widget", "switcher"],
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^8.0",
        "laracasts/flash": "^3.0",
        "pwweb/laravel-core": "^0.1.0-beta",
        "fzaninotto/faker": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
        "psr-4": {
            "PWWEB\\Localisation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PWWEB\\Localisation\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "PWWEB\\Localisation\\LocalisationServiceProvider"
            ],
            "aliases": {
                "Localisation": "PWWEB\\Localisation\\Facades\\Localisation"
            }
        }
    }
}